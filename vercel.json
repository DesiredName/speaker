{
	"version": 2,
	"rewrites": [{
		"source": "/(.*)",
		"destination": "/api"
	}],
	"headers":  [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/speaker-opal.vercel.app/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }  
    ]
}
